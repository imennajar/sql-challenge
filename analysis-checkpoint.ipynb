   "metadata": {},
   "source": [
    "--List the frequency counts, in descending order, of all the employee last names (that is, how many employees share each last name)--\n",
    "SELECT last_name, COUNT(last_name) FROM employees \n",
    "group by last_name\n",
    "order by last_name;"
   ]
